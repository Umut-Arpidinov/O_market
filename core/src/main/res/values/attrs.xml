<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumberInputView">
        <attr name="cancelImageVisibility" format="boolean" />
        <attr name="helperTextState" format="string" />
    </declare-styleable>

    <declare-styleable name="OtpInputCustomView">
        <attr name="otp_custom_view" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomRoundedOneCellLineView">
        <attr name="position" format="enum">
            <enum name="SINGLE" value="0"/>
            <enum name="TOP" value="1"/>
            <enum name="MIDDLE" value="2"/>
            <enum name="BOTTOM" value="3"/>
        </attr>
        <attr name="setIcon" format="string" />
        <attr name="setTitle" format="integer" />
        <attr name="setInfo" format="string" />
        <attr name="isShevronVisible" format="boolean"/>
        <attr name="isSimpleCell" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomAddPriceCellView" parent="CustomRoundedOneTitleTextCellsView">
        <attr name="setPriceWithoutCoins" format="string"/>
        <attr name="setPriceWithCoins" format="string"/>
        <attr name="setIcon" format="string"/>
        <attr name="setTitle" />
        <attr name="isNumberVerified" format="boolean"/>
        <attr name="isODengiAccepted" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomTwoCellsLineView" parent="CustomSmallSubtitleTextCellsView">
        <attr name="setSubtitle" />
        <attr name="setIcon" format="string"/>
        <attr name="setTitle" />
        <attr name="setDetails" />
    </declare-styleable>

    <declare-styleable name="CustomCompositeCellView">
        <attr name="setIcon" />
        <attr name="setDetails" />
        <attr name="setDate" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomDividerCellView">
        <attr name="setTitle" />
        <attr name="setButton" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomFAQCellView">
        <attr name="setQuestions" format="string" />
        <attr name="setAnswers" format="string"/>
    </declare-styleable>

    <declare-styleable name="ProgressButtonView">
        <attr name="text" format="string" />
    </declare-styleable>
</resources>