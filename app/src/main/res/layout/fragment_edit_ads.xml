<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_4"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".presentation.ui.fragments.edit_ads.EditAdsFragment">

    <kg.o.internlabs.core.custom_views.cells.CustomAddPriceCellView
        android:id="@+id/cus_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_12dp"
        android:layout_marginHorizontal="@dimen/margin_horizontal_12dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <kg.o.internlabs.core.custom_views.cells.CustomFAQCellView
        android:id="@+id/cus_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_12dp"
        app:layout_constraintTop_toBottomOf="@id/cus_1"/>

    <kg.o.internlabs.core.custom_views.cells.CustomOneCellView
        android:id="@+id/cus_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_12dp"
        app:layout_constraintTop_toBottomOf="@id/cus_2"/>

    <kg.o.internlabs.core.custom_views.cells.CustomProfileCellView
        android:id="@+id/cus_4"
        android:layout_width="match_parent"
        android:layout_margin="@dimen/margin_12dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/cus_3"/>

    <kg.o.internlabs.core.custom_views.cells.CustomRoundedOneCellLineView
        android:id="@+id/cus_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_12dp"
        app:layout_constraintTop_toBottomOf="@id/cus_4"/>

    <kg.o.internlabs.core.custom_views.cells.CustomWithToggleCellView
        android:id="@+id/cus_6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_12dp"
        app:layout_constraintTop_toBottomOf="@id/cus_5"/>

</androidx.constraintlayout.widget.ConstraintLayout>