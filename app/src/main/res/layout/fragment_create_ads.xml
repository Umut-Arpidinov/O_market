<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:background="@color/gray_4"
    android:layout_height="match_parent"
    tools:context=".presentation.ui.fragments.ads.CreateAdsFragment">

    <kg.o.internlabs.core.custom_views.cells.CustomRoundedOneCellLineView
        android:id="@+id/cus1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:position="TOP"
        android:layout_marginHorizontal="@dimen/margin_8dp"
        android:layout_marginTop="@dimen/margin_12dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <kg.o.internlabs.core.custom_views.cells.CustomRoundedOneCellLineView
        android:id="@+id/cus2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:position="MIDDLE"
        android:layout_marginHorizontal="@dimen/margin_8dp"
        app:layout_constraintTop_toBottomOf="@id/cus1">

    </kg.o.internlabs.core.custom_views.cells.CustomRoundedOneCellLineView>

    <kg.o.internlabs.core.custom_views.cells.CustomRoundedOneCellLineView
        android:id="@+id/cus3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin_8dp"
        app:layout_constraintTop_toBottomOf="@id/cus2"
        app:position="MIDDLE"/>

    <kg.o.internlabs.core.custom_views.cells.CustomRoundedOneCellLineView
        android:id="@+id/cus4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin_8dp"
        app:layout_constraintTop_toBottomOf="@id/cus3"
        app:position="BOTTOM"/>
</androidx.constraintlayout.widget.ConstraintLayout>