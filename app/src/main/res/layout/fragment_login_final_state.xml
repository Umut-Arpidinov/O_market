<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:background="@color/gray_4"
    android:layout_height="match_parent"
    tools:context=".presentation.ui.fragments.login.LoginInitialStateFragment">

    <include
        android:id="@+id/included"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="wrap_content"
        layout="@layout/custom_one_cell_view"
        app:layout_constraintTop_toTopOf="parent"/>
    <kg.o.internlabs.core.custom_views.cells.CustomOneCellView
        android:id="@+id/cccv1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:visibility="gone"
        app:isEditableView="false"
        app:setDelimiter="14"
        app:setText="string/cells_info khkhgbkbk hkkft gfffb hjkkn tre tre trw 35664 786853 84768463 "
        app:layout_constraintTop_toTopOf="parent"/>

    <kg.o.internlabs.core.custom_views.cells.CustomOneCellView
        android:id="@+id/cccv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:visibility="visible"
        app:isEditableView="false"
        app:setDelimiter="7"
        app:layout_constraintTop_toBottomOf="@id/cccv1"
        app:setHint="string/cells_info khkhgbkbk hkkft gfffb hjkkn tre tre trw tre tre tre tre tre trew "/>

    <kg.o.internlabs.core.custom_views.cells.CustomAddPriceCellView
        android:id="@+id/csstcv_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin_16dp"
        app:layout_constraintTop_toBottomOf="@id/cccv"
        android:layout_marginTop="15dp"
        app:setPriceWithoutCoins="$77."
        app:setPriceWithCoins="47"
        app:isNumberVerified="true"
        app:isODengiAccepted="true"
        app:setIcon="@drawable/bg_cells_image"
        app:setTitle="Мадина" />

    <kg.o.internlabs.core.custom_views.cells.CustomRoundedOneCellLineView
        android:id="@+id/cottcv_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_8dp"
        android:layout_marginHorizontal="@dimen/margin_16dp"
        app:layout_constraintTop_toBottomOf="@id/csstcv_1"
        app:position ="SINGLE"
        app:isShevronVisible="true"
        app:setIcon="@drawable/bg_cells_image"
        app:setTitle="@string/title_in_cells" />

    <kg.o.internlabs.core.custom_views.cells.CustomProfileCellView
        android:id="@+id/cbstcv_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_8dp"
        android:layout_marginHorizontal="@dimen/margin_16dp"
        app:layout_constraintTop_toBottomOf="@id/cottcv_2"
        app:position="BOTTOM"
        app:setIcon="@drawable/bg_cells_image"
        app:setSubtitle="г. Бишкек"
        app:setTitle="+996 700 000 104" />


    <kg.o.internlabs.core.custom_views.cells.CustomWithToggleCellView
        android:id="@+id/cdcv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_horizontal_8dp"
        android:visibility="visible"
        app:layout_constraintTop_toBottomOf="@id/cbstcv_1"
        app:setTitle="@string/title_in_cells" />

    <kg.o.internlabs.core.custom_views.cells.CustomFAQCellView
        android:id="@+id/cfaqcv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:setQuestions="@string/questions"
        android:layout_marginTop="@dimen/margin_horizontal_8dp"
        app:setAnswers="@string/answer"
        app:layout_constraintTop_toBottomOf="@id/cdcv"/>

</androidx.constraintlayout.widget.ConstraintLayout>