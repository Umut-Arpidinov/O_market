<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".presentation.ui.fragments.profile.ProfileFragment">

    <kg.o.internlabs.core.custom_views.cells.CustomProfileCellView
        android:id="@+id/cus_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:setTitle="+996 776 543 654"
        app:setSubtitle="г. Токмок"
        android:layout_marginTop="@dimen/margin_top_11dp"
        android:layout_marginHorizontal="@dimen/margin_horizontal_13dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ProfileFragment \n Экран профиля"
        android:textSize="30sp"
        android:layout_marginTop="@dimen/margin_top_40dp"
        app:layout_constraintTop_toBottomOf="@id/cus_profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/what"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="нажмите сюда, если не поняли"
        android:gravity="bottom"
        android:textSize="30sp"
        android:clickable="true"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:focusable="true" />

</androidx.constraintlayout.widget.ConstraintLayout>